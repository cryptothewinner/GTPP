{
    "name": "sepe-natural-2026",
    "version": "0.1.0",
    "private": true,
    "packageManager": "pnpm@9.15.0",
    "scripts": {
        "dev": "turbo dev",
        "build": "turbo build",
        "lint": "turbo run type-check --filter=@sepe/web --filter=@sepenatural/shared",
        "db:generate": "pnpm --filter @sepenatural/api prisma generate",
        "db:migrate": "pnpm --filter @sepenatural/api prisma migrate dev",
        "db:seed": "pnpm --filter @sepenatural/api prisma db seed",
        "db:studio": "pnpm --filter @sepenatural/api prisma studio",
        "clean": "turbo clean && rm -rf node_modules"
    },
    "devDependencies": {
        "eslint": "^9.0.0",
        "prettier": "^3.4.0",
        "turbo": "^2.3.0",
        "typescript": "^5.7.0"
    },
    "dependencies": {
        "@dnd-kit/core": "^6.3.1",
        "@dnd-kit/sortable": "^10.0.0",
        "@dnd-kit/utilities": "^3.2.2",
        "@types/react-big-calendar": "^1.16.3",
        "date-fns": "^4.1.0",
        "lucide-react": "^0.468.0",
        "react-big-calendar": "^1.19.4",
        "recharts": "^3.7.0"
    }
}
